<% if current_user.nil?%>
 	<%= link_to image_tag('facebook-login.png'), "/auth/facebook" %>
<% else %>
	<%= link_to image_tag(current_user.avatar_url, :class => 'avatar_url') + ' ' + current_user.nickname, "#", :class => "remove-right-border left-border" %> | <%= link_to "Log af", logout_path %>
<% end %>

