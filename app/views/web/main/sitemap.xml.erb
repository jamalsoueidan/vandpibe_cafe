<?xml version="1.0" encoding="UTF-8"?>
<urlset
      xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">

<url>
	<loc>http://www.vandpibecafe.dk/</loc>
	<changefreq>monthly</changefreq>
	<priority>0.5</priority>
</url>
<%- @cities.each do |city| %>
<url>
	<loc>http://www.vandpibecafe.dk<%=city_path(city.name)%></loc>
	<changefreq>monthly</changefreq>
	<priority>0.8</priority>
</url>
<%- city.locations.each do |location|%>
<url>
	<loc>http://www.vandpibecafe.dk<%=city_location_path(city.name, location.name)%></loc>
	<changefreq>daily</changefreq>
	<priority>1</priority>
</url>
<%- end %>
<%- end %>
<url>
	<loc>http://www.vandpibecafe.dk<%=questions_path%></loc>
	<changefreq>daily</changefreq>
	<priority>0.5</priority>
</url>
<%- @questions.each do |question| %>
<url>
	<loc>http://www.vandpibecafe.dk<%=question_path(question.id, question.title.parameterize)%></loc>
	<changefreq>daily</changefreq>
	<priority>1</priority>
</url>
<%- end %>
<url>
  <loc>http://www.vandpibecafe.dk/toplist</loc>
  <changefreq>monthly</changefreq>
  <priority>1</priority>
</url>
<url>
  <loc>http://www.vandpibecafe.dk/toplist/bedste+vandpibe</loc>
  <changefreq>monthly</changefreq>
  <priority>1</priority>
</url>
<url>
  <loc>http://www.vandpibecafe.dk/toplist/bedste+service</loc>
  <changefreq>monthly</changefreq>
  <priority>1</priority>
</url>
<url>
  <loc>http://www.vandpibecafe.dk/toplist/bedste+indretning</loc>
  <changefreq>monthly</changefreq>
  <priority>1</priority>
</url>
<url>
  <loc>http://www.vandpibecafe.dk/toplist/bedste+stemning</loc>
  <changefreq>monthly</changefreq>
  <priority>1</priority>
</url>
</urlset>
