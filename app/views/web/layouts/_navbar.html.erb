<div class="row" style="margin-bottom:10px;">
  <div class="four columns">
    <%= link_to(image_tag('logo3.png'), '/', :alt => 'Vandpibe Cafe')%>
  </div>
  <div class="eight columns navigation">
    <div class="login">
      <%= javascript_tag :login do %>
        $(function() {
          $.get('<%=authjs_path%>', function(data) {
            $('.login').html(data)
          });
        })
      <% end %>
    </div>

    <ul class="block-grid three-up mobile-one-up">
      <li><%= link_to(image_tag('vandpibe_cafeer.png'), '/', :alt => 'Vandpibe Cafe')%></li>
      <li><%= link_to(image_tag('debat.png'), questions_path, :alt => 'Debat')%></li>
      <li><%= link_to(image_tag('top10.png'), toplist_path, :alt => 'Bedste Vandpibe Cafeer')%></li>
    </ul>
  </div>
</div>
